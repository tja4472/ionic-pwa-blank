{
  "hosting": {
    "public": "www",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
/*   
			{
        "source": "service-worker.js",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "no-cache"
        }]
      }
*/
// "value": "max-age=0"
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}
